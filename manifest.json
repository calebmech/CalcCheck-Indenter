{
  "name": "CalcCheck Indenter",
  "version": "1.4",
  "description": "Easy indenting for CalcCheckWeb",
  "manifest_version": 2,
  "permissions": ["*://130.113.68.214/*"],
  "content_scripts": [
    {
      "matches": ["*://130.113.68.214/*"],
      "run_at": "document_idle",
      "js": ["script.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
